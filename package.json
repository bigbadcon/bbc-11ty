{
    "name": "bbc-11ty",
    "version": "0.0.2",
    "description": "Big Bad Con 11ty site",
    "author": {
        "name": "Colin Fahrion",
        "url": "https://colinaut.com"
    },
    "main": ".eleventy.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/kkgthb/11ty-tailwind-jit.git"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "concurrently \"npm run eleventybuild\" \"npm run esbuild\"",
        "eleventybuild": "cross-env ELEVENTY_ENV=prod eleventy",
        "esbuild": "node esbuild.js",
        "dev": "cross-env ELEVENTY_ENV=dev eleventy --serve --quiet",
        "develop": "eleventy --serve",
        "debug": "DEBUG=* eleventy",
        "watch": "node esbuild.js --watch",
        "svgs": "npx svg-sprite-generate -d src/assets/svgs -o src/static/images/icons.svg",
        "gd": "git checkout drafts && git merge main",
        "gm": "git checkout main && git merge drafts"
    },
    "dependencies": {
        "@11ty/eleventy": "latest",
        "@11ty/eleventy-cache-assets": "^2.2.1",
        "@11ty/eleventy-img": "^0.9.0",
        "@11ty/eleventy-plugin-rss": "^1.1.1",
        "@alpinejs/intersect": "^3.10.3",
        "@alpinejs/persist": "^3.10.3",
        "@colinaut/alpinejs-plugin-simple-validate": "^1.7.12",
        "@netlify/functions": "^0.7.2",
        "@sendgrid/mail": "^7.4.6",
        "@slack/bolt": "^3.5.0",
        "@tailwindcss/aspect-ratio": "^0.4.0",
        "@tailwindcss/forms": "^0.5.2",
        "@tailwindcss/typography": "^0.5.2",
        "airtable": "^0.11.4",
        "alpinejs": "^3.10.3",
        "array-flat-polyfill": "^1.0.1",
        "autoprefixer": "latest",
        "axios": "^0.21.1",
        "calendar-link": "^2.1.1",
        "chrome-aws-lambda": "^10.1.0",
        "cross-env": "^7.0.3",
        "dayjs": "^1.10.7",
        "dotenv": "^10.0.0",
        "eleventy-plugin-blog-tools": "^0.3.1",
        "fast-glob": "^3.2.12",
        "google-spreadsheet": "^3.1.15",
        "lite-youtube-embed": "^0.2.0",
        "markdown-it": "^13.0.1",
        "markdown-it-attrs": "^4.1.4",
        "netlify-plugin-inline-functions-env": "^1.0.9",
        "node-fetch": "^3.1.0",
        "postcss": "latest",
        "postcss-cli": "latest",
        "puppeteer-core": "^10.2.0",
        "slugify": "^1.6.0",
        "ssl-root-cas": "^1.3.1",
        "stripe": "^10.3.0",
        "tailwindcss": "latest",
        "utf8": "^3.0.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@playwright/test": "^1.22.2",
        "concurrently": "^7.4.0",
        "esbuild": "^0.15.10",
        "eslint": "^8.24.0",
        "svg-sprite-generator": "^0.0.7"
    }
}