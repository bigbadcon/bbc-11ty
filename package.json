{
  "name": "bbc-11ty",
  "version": "0.0.2",
  "description": "Big Bad Con 11ty site",
  "author": {
    "name": "Colin Fahrion",
    "url": "https://colinaut.com"
  },
  "main": ".eleventy.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kkgthb/11ty-tailwind-jit.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
