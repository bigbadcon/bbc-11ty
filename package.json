{
	"name": "bbc-11ty",
	"version": "0.0.2",
	"description": "Big Bad Con 11ty site",
	"author": {
		"name": "Colin Fahrion",
		"url": "https://colinaut.com"
	},
	"main": ".eleventy.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/kkgthb/11ty-tailwind-jit.git"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "npm-run-all -p dev:*",
		"dev": "npm-run-all -p dev:*",
		"dev:tinacms": "tinacms dev",
		"dev:11ty": "cross-env ELEVENTY_ENV=dev eleventy --serve --quiet",
		"dev:js": "node esbuild.js --watch",
		"dev_lite": "run-s dev:11ty dev:js",
		"build": "run-s build:*",
		"build:tinacms": "tinacms build",
		"build:11ty": "cross-env ELEVENTY_ENV=prod eleventy",
		"build:js": "node esbuild.js",
		"build_lite": "run-s build:11ty build:js",
		"debug": "DEBUG=* eleventy",
		"svgs": "npx svg-sprite-generate -d src/assets/svgs -o src/static/images/icons.svg",
		"gd": "git checkout drafts && git merge main",
		"gm": "git checkout main && git merge drafts",
		"tinacms:audit": "tinacms audit",
		"clear": "rm -rf .cache"
	},
	"dependencies": {
		"@11ty/eleventy": "^2.0.1",
		"@11ty/eleventy-fetch": "^4.0.0",
		"@11ty/eleventy-img": "^3.1.1",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@alpinejs/intersect": "^3.10.3",
		"@alpinejs/persist": "^3.10.3",
		"@colinaut/action-form": "~0.6.5",
		"@colinaut/action-table": "^2.4.15",
		"@colinaut/alpinejs-plugin-simple-validate": "^1.7.20",
		"@colinaut/theme-multi-switch": "^0.8.2",
		"@netlify/functions": "^2.2.1",
		"@netlify/node-cookies": "^0.1.0",
		"@netlify/serverless-functions-api": "^1.8.0",
		"@sendgrid/mail": "^7.7.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.12",
		"@tinacms/cli": "^1.5.29",
		"alpinejs": "^3.13.1",
		"array-flat-polyfill": "^1.0.1",
		"autoprefixer": "latest",
		"axios": "^1.5.1",
		"calendar-link": "^2.6.0",
		"chrome-aws-lambda": "^10.1.0",
		"cross-env": "^7.0.3",
		"dayjs": "^1.10.7",
		"dotenv": "^16.3.1",
		"eleventy-plugin-add-web-component-definitions": "^2.2.0",
		"eleventy-plugin-blog-tools": "^0.4.1",
		"esbuild": "^0.19.4",
		"fast-glob": "^3.3.1",
		"fs-extra": "^11.1.0",
		"google-auth-library": "^8.9.0",
		"google-spreadsheet": "^4.1.0",
		"install": "^0.13.0",
		"lite-youtube-embed": "^0.3.0",
		"markdown-it": "^13.0.2",
		"markdown-it-anchor": "^8.6.7",
		"markdown-it-attrs": "^4.1.6",
		"netlify-plugin-inline-functions-env": "^1.0.9",
		"node-fetch": "^3.3.2",
		"npm-run-all": "^4.1.5",
		"postcss": "latest",
		"postcss-cli": "latest",
		"puppeteer-core": "^10.2.0",
		"qs": "^6.11.2",
		"slugify": "^1.6.6",
		"ssl-root-cas": "^1.3.1",
		"stripe": "^13.9.0",
		"tailwindcss": "latest",
		"tinacms": "^1.5.21",
		"urlpattern-polyfill": "^9.0.0",
		"utf8": "^3.0.0",
		"uuid": "^9.0.1",
		"windows-1252": "1.1.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.22.2",
		"eslint": "^8.24.0",
		"husky": "^8.0.3",
		"netlify-cli": "^17.15.1"
	}
}
