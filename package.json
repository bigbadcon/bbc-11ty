{
	"name": "bbc-11ty",
	"version": "0.0.2",
	"description": "Big Bad Con 11ty site",
	"author": {
		"name": "Colin Fahrion",
		"url": "https://colinaut.com"
	},
	"main": ".eleventy.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/kkgthb/11ty-tailwind-jit.git"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "npm-run-all -p dev:*",
		"dev": "npm-run-all -p dev:*",
		"dev:tinacms": "tinacms dev",
		"dev:11ty": "cross-env ELEVENTY_ENV=dev eleventy --serve --quiet",
		"dev:js": "node esbuild.js --watch",
		"build": "run-s build:*",
		"build:tinacms": "tinacms build",
		"build:11ty": "cross-env ELEVENTY_ENV=prod eleventy",
		"build:js": "node esbuild.js",
		"debug": "DEBUG=* eleventy",
		"svgs": "npx svg-sprite-generate -d src/assets/svgs -o src/static/images/icons.svg",
		"gd": "git checkout drafts && git merge main",
		"gm": "git checkout main && git merge drafts",
		"tinacms:audit": "tinacms audit"
	},
	"dependencies": {
		"@11ty/eleventy": "^2.0.0",
		"@11ty/eleventy-fetch": "^4.0.0",
		"@11ty/eleventy-img": "^3.0.0",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@alpinejs/intersect": "^3.10.3",
		"@alpinejs/persist": "^3.10.3",
		"@colinaut/alpinejs-plugin-simple-validate": "^1.7.16",
		"@netlify/functions": "^0.7.2",
		"@sendgrid/mail": "^7.4.6",
		"@slack/bolt": "^3.5.0",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/typography": "^0.5.2",
		"@tinacms/cli": "^1.5.6",
		"airtable": "^0.11.4",
		"alpinejs": "^3.10.3",
		"array-flat-polyfill": "^1.0.1",
		"autoprefixer": "latest",
		"axios": "^0.21.1",
		"calendar-link": "^2.1.1",
		"chrome-aws-lambda": "^10.1.0",
		"cross-env": "^7.0.3",
		"dayjs": "^1.10.7",
		"dotenv": "^10.0.0",
		"eleventy-plugin-blog-tools": "^0.3.1",
		"esbuild": "^0.15.10",
		"fast-glob": "^3.2.12",
		"fs-extra": "^11.1.0",
		"google-spreadsheet": "^3.1.15",
		"install": "^0.13.0",
		"lite-youtube-embed": "^0.2.0",
		"markdown-it": "^13.0.1",
		"markdown-it-anchor": "^8.6.7",
		"markdown-it-attrs": "^4.1.4",
		"netlify-plugin-inline-functions-env": "^1.0.9",
		"node-fetch": "^3.1.0",
		"npm": "^9.4.1",
		"npm-run-all": "^4.1.5",
		"postcss": "latest",
		"postcss-cli": "latest",
		"puppeteer-core": "^10.2.0",
		"slugify": "^1.6.0",
		"ssl-root-cas": "^1.3.1",
		"stripe": "^10.3.0",
		"tailwindcss": "latest",
		"tinacms": "^1.5.2",
		"utf8": "^3.0.0",
		"uuid": "^8.3.2",
		"windows-1252": "1.1.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.22.2",
		"eslint": "^8.24.0",
		"svg-sprite-generator": "^0.0.7"
	}
}
